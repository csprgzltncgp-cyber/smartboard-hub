{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "amrshawky/laravel-currency": "^6.0",
    "bacon/bacon-qr-code": "^2.0",
    "barryvdh/laravel-dompdf": "^3.0",
    "diglactic/laravel-breadcrumbs": "^8.0",
    "doctrine/dbal": "^3.0",
    "fakerphp/faker": "^1.9.1",
    "guzzlehttp/guzzle": "^7.5",
    "harvesthq/chosen": "^1.8",
    "iio/libmergepdf": "^4.0",
    "laravel-notification-channels/pusher-push-notifications": "^4.2",
    "laravel/framework": "^10.0",
    "laravel/telescope": "^4.16",
    "laravel/tinker": "^2.10",
    "laravel/vonage-notification-channel": "^3.2",
    "league/flysystem-sftp-v3": "^3.0",
    "livewire/livewire": "^2.11",
    "maatwebsite/excel": "^3.1",
    "openai-php/laravel": "^0.8.1",
    "pragmarx/google2fa-laravel": "^2.0",
    "pusher/pusher-php-server": "^7.2",
    "spatie/flysystem-dropbox": "2.0.6",
    "spatie/laravel-activitylog": "^4.7",
    "spatie/laravel-ignition": "^2.0",
    "twilio/sdk": "^7.3",
    "vimeo/laravel": "^5.10",
    "wire-elements/modal": "^1.0",
    "zoparga/laravel-szamlazzhu": "^1.3"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.13",
    "driftingly/rector-laravel": "^0.26.0",
    "larastan/larastan": "^2.8",
    "laravel/boost": "^1.8",
    "laravel/pint": "^1.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^6.1",
    "phpunit/phpunit": "^9.5.10",
    "rector/rector": "^0.19.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "php-http/discovery": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "analyze": [
        "./vendor/bin/phpstan analyse --memory-limit=4G"
    ],
    "rector" : [
        "./vendor/bin/rector"
    ],
    "format": [
        "./vendor/bin/pint"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ]
  }
}
